<h2 class="container-heading match-heading">
  <i class="ms-Icon ms-Icon--Soccer"></i>
  Match center
</h2>
<ul class="match-list">
  <% matches.forEach(match => { %>
  <li class="match">
    <a href="/match/<%= match.id %>">
      <p class="match-team match-home--team"><%= match.homeTeam.name %></p>
      <p class="match-team match-away--team"><%= match.awayTeam.name %></p>
    </a>
  </li>
  <% }) %>
</ul>
